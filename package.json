{
  "name": "jsutils",
  "version": "2.2.0",
  "description": "common javascript utils",
  "main": "cjs/",
  "module": "esm/",
  "repository": "https://github.com/lancetipton/js-helpers.git",
  "author": "lancetipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ENV=development webpack --watch --progress",
    "build": "yarn build:clean && yarn build:webpack",
    "build:clean": "rm -rf ./esm && rm -rf ./cjs && rm -rf ./build",
    "build:esm": "babel --delete-dir-on-start -d esm/ src/scripts/",
    "build:cjs": "babel --delete-dir-on-start --env-name cjs -d cjs/ src/scripts/",
    "build:modules": "yarn build:esm && yarn build:cjs",
    "build:webpack": "rm -rf ./build && ENV=production webpack --mode production",
    "clean": "rm -rf yarn.lock && rm -rf node_modules && yarn install --force",
    "deploy": "gh-pages -d build",
    "test": "NODE_ENV=test jest --verbose"
  },
  "dependencies": {
    "webpack-shell-plugin": "https://github.com/cdeutsch/webpack-shell-plugin.git#bee537d"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-property-literals": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.5",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^3.1.2",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "raw-loader": "^2.0.0",
    "remove-comments-loader": "^0.1.2",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/index.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "es6"
    ],
    "globals": {
      "__DEV__": true
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
